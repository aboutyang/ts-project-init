{
  // Workspace settings
  "editor.fontSize": 14,
  "eslint.format.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.probe": ["javascript", "javascriptreact", "vue"],
  // eslint.validate tells the ESLint extension which languages it should check.
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "vue",
    "json"
  ],
  // editor.codeActionsOnSave is a hook into VS Codeâ€™s save event.
  "editor.codeActionsOnSave": {
    // source.fixAll.eslint says to fix any issues in the file being saved.
    "source.fixAll.eslint": "explicit"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": []
}
